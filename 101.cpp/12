int main() {
    fast
   string s;
    cin>>s;
    vector<int>v;
    for(int i=0;i<s.size();i+=2)
    {
       int x=s[i]-'0';
       v.push_back(x);
 
    }
    sort(v.begin(),v.end());
   for(int i=0;i<v.size();i++)
   {
       cout<<v[i];
       if(i!=v.size()-1)
       {
           cout<<"+";
       }
   }
}

