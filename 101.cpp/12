//#include <bits>
using namespace std;
const int MAXN = 1e5;
int main() {
	ios::sync_with_stdio(false) , cin.tie(0) , cout.tie(0);
	string s;
	int ones = 0 , twos = 0 , threes = 0;
	cin >> s;
	for(int i = 0 ; i < s.size() ; i++)
	{
		if(s[ i ] == '1')
			ones++;
		if(s[ i ] == '2')
			twos++;
		if(s[ i ] == '3')
			threes++;	
	}
	if(ones != 0)
	{
		cout << 1;
		for(int i = 0 ; i < ones - 1 ; i++)
			cout << '+' << 1;
		for(int i = 0 ; i < twos  ; i++)
			cout << '+' << 2;
		for(int i = 0 ; i < threes ; i++)
			cout << '+' << 3;	
		return 0;	
	}
	if(twos != 0)
	{
		cout << 2;
		for(int i = 0 ; i < twos - 1 ; i++)
			cout << '+' << 2;
		for(int i = 0 ; i < threes ; i++)
			cout << '+' << 3;	
		return 0;	
	}
	if(threes != 0)
	{
		cout << 3;
		for(int i = 0 ; i < threes - 1; i++)
			cout << '+' << 3;
		return 0;	
	}
}
#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
 
int main() {
	string s;
	vector<int> ans;
	cin >> s;
	for (int i = 0; i < s.size(); i++) {
		if (s[i] >= '0' && s[i] <= '9')
			ans.push_back(s[i] - '0');
	}
	sort(ans.begin(), ans.end());
	cout << ans[0];
	for (int i = 1; i < ans.size(); i++) {
		cout << "+" << ans[i];
	}
	return 0;
}
int main() {
//    solve1();
    string s;
    cin >> s;
    vector<char> z;
    for (ll i = 0; i < s.size(); i++) {
        if (s[i] == '1' or s[i] == '2' or s[i] == '3') {
            z.push_back(s[i]);
        }
    }
    sort(z.begin(), z.end());
    for (ll i = 0; i < z.size(); i++) {
        if (i != 0) {
            cout << "+" << z[i];
        } else {
            cout << z[i];
        }
    }
}
