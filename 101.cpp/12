#include <iostream>
#include <string>
#include <algorithm>
using namespace std;
int main()
{
    string given;
    int i;
    cin >> given;
    sort(given.begin(), given.end());
    for (i = (given.length() - 1) / 2; i < given.length() - 1; i++)
        cout << given[i] << "+";
    cout << given[i];
    return 0;
}
include<iostream>
#include<cstdio>
#include<cstring>
#include<algorithm>
using namespace std;
const int N = 10000;
char a[N];
char num[N];
int main()
{
    int n,m,i,j,ans;
    while(~scanf("%s",a))
    {
        memset(num,0,sizeof(num));
        int len = strlen(a);
        int pos = 0;
        for(i = 0; i < len; i++)
        {
            if(a[i] >= '0' && a[i] <= '3')
            {
                num[pos++] = a[i];
            }
        }
        sort(num,num+pos);
        printf("%c",num[0]);
        for(i = 1; i < pos;i++)
        {
            printf("+%c",num[i]);
        }
        puts("");
    }
    return 0;
}
void solve()
{
	int a[200];
string s; cin>>s;
int len=s.length()/2+1;
for(int i=0;i<len;i++)
a[i]=s[2*i]-'0';
sort(a,a+len);
for(int i=0;i<len-1;i++)
cout<<a[i]<<'+';
cout<<a[len-1];
}
